<h1>Listing contacts</h1>

<table>
  <tr>
    <th>First</th>
    <th>Last</th>
    <th>Company</th>
    <th>Phone</th>
    <th>Url</th>
    <th>Chat</th>
    <th>Street</th>
    <th>City</th>
    <th>State</th>
    <th>Zip</th>
    <th>Country</th>
    <th>Notes</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @contacts.each do |contact| %>
  <tr>
    <td><%= contact.first %></td>
    <td><%= contact.last %></td>
    <td><%= contact.company %></td>
    <td><%= contact.phone %></td>
    <td><%= contact.url %></td>
    <td><%= contact.chat %></td>
    <td><%= contact.street %></td>
    <td><%= contact.city %></td>
    <td><%= contact.state %></td>
    <td><%= contact.zip %></td>
    <td><%= contact.country %></td>
    <td><%= contact.notes %></td>
    <td><%= link_to 'Show', contact %></td>
    <td><%= link_to 'Edit', edit_contact_path(contact) %></td>
    <td><%= link_to 'Destroy', contact, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Contact', new_contact_path %>
